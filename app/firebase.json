{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "functions": {
    "predeploy": [
    ]
  },
  "hosting": {
    "predeploy":"npm run build",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
              "source": "/timeline", "function": "timeline"
      },
      {
              "source": "/getuser", "function": "getuser"
      }
    ]
  }
}
